{
  "compilerOptions": {
    "noErrorTruncation": true,
    "typeRoots": [
      "node_modules/@types",
      "src/types"
    ],
    "jsx": "preserve",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "module": "esnext",
    "moduleResolution": "node",
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "downlevelIteration": true,
    "baseUrl": "src",
    "paths": {
      "@assets/*": [
        "./src/assetsassets/*"
      ]
    },
    "incremental": true,
    "target": "es5"
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "./src",
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "next.config.js"
  ]
}